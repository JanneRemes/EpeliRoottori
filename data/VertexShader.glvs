#version 330 core

//Defined also in vertexshader.h
#define VERTEX_POSITION 0

layout(location = VERTEX_POSITION) in vec3 vertexPosition_modelspace;
uniform mat4 MVP;

/* system outputs
vec4 gl_Position;
float gl_PointSize;
float gl_ClipDistance[];
*/


void main() {
  gl_Position =  MVP * vec4(vertexPosition_modelspace,1);
}
